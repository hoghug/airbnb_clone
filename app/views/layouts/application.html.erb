<!DOCTYPE html>
<html>
<head>
  <title>Bearbnb</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/header' %>


  <div id="wrapper" class="container">

    <% if flash[:alert] %>
      <%= flash[:alert] %>
    <% end %>
    <% if flash[:notice] %>
      <%= flash[:notice] %>
    <% end %>

    <%= yield %>
  </div>

</body>
</html>
